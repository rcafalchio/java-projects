package com.leilaopecuario.vo;

public class DadosLanceUsuarioVO {

	private UsuarioVO usuarioVO;
	private LeilaoVO leilaoVO;
	private Double lanceEfetuado;
	private boolean lanceGanhador;

	public UsuarioVO getUsuarioVO() {
		return usuarioVO;
	}

	public void setUsuarioVO(UsuarioVO usuarioVO) {
		this.usuarioVO = usuarioVO;
	}

	public LeilaoVO getLeilaoVO() {
		return leilaoVO;
	}

	public void setLeilaoVO(LeilaoVO leilaoVO) {
		this.leilaoVO = leilaoVO;
	}

	public Double getLanceEfetuado() {
		return lanceEfetuado;
	}

	public void setLanceEfetuado(Double lanceEfetuado) {
		this.lanceEfetuado = lanceEfetuado;
	}

	public boolean isLanceGanhador() {
		return lanceGanhador;
	}

	public void setLanceGanhador(boolean lanceGanhador) {
		this.lanceGanhador = lanceGanhador;
	}

}
