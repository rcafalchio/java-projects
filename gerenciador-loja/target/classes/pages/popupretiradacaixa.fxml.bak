<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?language javascript?>
<?scenebuilder-stylesheet ../styles/style_tecway.css?>
<?scenebuilder-background-color 0x708090ff?>
<!--<?import br.com.tecway.gerenciadorloja.fx.components.NumberTextField?>-->

<StackPane fx:id="root" prefHeight="310.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml" fx:controller="br.com.tecway.gerenciadorloja.fx.controller.RetiradaController">
  <children>
    <VBox prefHeight="310.0" prefWidth="800.0">
      <children>
        <HBox alignment="TOP_CENTER" prefHeight="81.0" prefWidth="705.0">
          <children>
            <Label styleClass="label-titulo" text="Retirada de dinheiro" />
          </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="74.0" prefWidth="800.0">
          <children>
            <GridPane prefHeight="125.0" prefWidth="275.0">
              <children>
                <Label styleClass="label-padrao" text="Dinheiro em caixa:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <Label fx:id="totalLabel" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <Label styleClass="label-padrao" text="Valor da retirada:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <Label fx:id="dinheiroLabel" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <TextField prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
              </children>
              <columnConstraints>
                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="168.0" minWidth="10.0" prefWidth="123.0" />
                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="208.0" minWidth="10.0" prefWidth="112.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
          </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
          <children>
            <ToolBar prefHeight="36.0" prefWidth="115.0">
              <items>
                <Button alignment="CENTER" mnemonicParsing="false" onAction="#fecharVenda" text="Efetuar retirada" />
              </items>
              <HBox.margin>
                <Insets />
              </HBox.margin>
            </ToolBar>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</StackPane>
